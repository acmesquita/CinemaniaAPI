<p id="notice"><%= notice %></p>

<h1>Sessaos</h1>

<table>
  <thead>
    <tr>
      <th>Inicio</th>
      <th>Dublado</th>
      <th>Tipo exibicao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessaos.each do |sessao| %>
      <tr>
        <td><%= sessao.inicio %></td>
        <td><%= sessao.dublado %></td>
        <td><%= sessao.tipo_exibicao %></td>
        <td><%= link_to 'Show', sessao %></td>
        <td><%= link_to 'Edit', edit_sessao_path(sessao) %></td>
        <td><%= link_to 'Destroy', sessao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sessao', new_sessao_path %>
